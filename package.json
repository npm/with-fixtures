{
  "name": "with-fixtures",
  "version": "1.1.0",
  "description": "A test fixture resource control tool",
  "main": "index.js",
  "scripts": {
    "test": "tap test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm/with-fixtures.git"
  },
  "keywords": [
    "promises",
    "test",
    "helper"
  ],
  "author": "Aria Stewart <aria@npmjs.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/npm/with-fixtures/issues"
  },
  "homepage": "https://github.com/npm/with-fixtures#readme",
  "devDependencies": {
    "tap": "^7.1.2"
  },
  "dependencies": {
    "bluebird": "^3.4.6"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8
    },
    "rules": {
      "valid-typeof": "error",
      "no-undef": "error",
      "no-unused-vars": "error"
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "engines": {
    "node": ">=4"
  }
}
